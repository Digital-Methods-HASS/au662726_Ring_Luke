@article{rhodesIntrinsicExtrinsicContributions2014,
  title = {Intrinsic and Extrinsic Contributions to Heavy Tails in Visual Foraging},
  author = {Rhodes, Theo and Kello, Christopher T. and Kerster, Bryan},
  date = {2014-07-03},
  journaltitle = {Visual Cognition},
  shortjournal = {Visual Cognition},
  volume = {22},
  number = {6},
  pages = {809--842},
  issn = {1350-6285, 1464-0716},
  doi = {10.1080/13506285.2014.918070},
  url = {http://www.tandfonline.com/doi/abs/10.1080/13506285.2014.918070},
  urldate = {2021-09-06},
  abstract = {Eyes move over visual scenes to gather visual information. Studies have found heavy-tailed distributions in measures of eye movements during visual search, which raises questions about whether these distributions are pervasive to eye movements, and whether they arise from intrinsic or extrinsic factors. Three different measures of eye movement trajectories were examined during visual foraging of complex images, and all three were found to exhibit heavy tails: Spatial clustering of eye movements followed a power law distribution, saccade length distributions were lognormally distributed, and the speeds of slow, small amplitude movements occurring during fixations followed a 1/f spectral power law relation. Images were varied to test whether the spatial clustering of visual scene information is responsible for heavy tails in eye movements. Spatial clustering of eye movements and saccade length distributions were found to vary with image type and task demands, but no such effects were found for eye movement speeds during fixations. Results showed that heavy-tailed distributions are general and intrinsic to visual foraging, but some of them become aligned with visual stimuli when required by task demands. The potentially adaptive value of heavy-tailed distributions in visual foraging is discussed.},
  langid = {english}
}

