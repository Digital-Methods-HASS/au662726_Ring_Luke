@article{realiWordsAllelesConnecting2010,
  title = {Words as Alleles: Connecting Language Evolution with {{Bayesian}} Learners to Models of Genetic Drift},
  shorttitle = {Words as Alleles},
  author = {Reali, Florencia and Griffiths, Thomas L.},
  date = {2010-02-07},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {277},
  number = {1680},
