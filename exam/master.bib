  shorttitle = {Agent-{{Based Modeling}}},
  author = {Jackson, Joshua Conrad and Rand, David and Lewis, Kevin and Norton, Michael I. and Gray, Kurt},
  date = {2017-05},
  journaltitle = {Social Psychological and Personality Science},
