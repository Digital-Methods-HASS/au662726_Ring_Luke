@article{parkanyiTheoreticalExperimentalDipole2002,
  title = {Theoretical and {{Experimental Dipole Moments}} of {{Purines}}},
  author = {Párkányi, Cyril and Boniface, Christian and Aaron, Jean-Jacques and Bulaceanu-MacNair, Mihaela and Dakkouri, Marwan},
  date = {2002},
